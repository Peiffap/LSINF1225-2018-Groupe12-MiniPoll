<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.PollEditActivity">

    <ImageView
        android:id="@+id/toHide1"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@android:color/holo_blue_dark" />

    <ImageView
        android:id="@+id/toHide3"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toHide1"
        app:srcCompat="@android:color/holo_blue_dark" />

    <ImageView
        android:id="@+id/toHide4"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toHide2"
        app:srcCompat="@android:color/holo_blue_dark" />

    <ImageView
        android:id="@+id/toHide2"
        android:layout_width="120dp"
